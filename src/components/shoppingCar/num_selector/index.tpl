<div class="num-selector" data-id="{{id}}" data-index={{index}}>
	<button class="item btn" data-field="minus">-</button>
	<input class="item input" type="text" value="{{num}}" readonly id="J_numInput_{{id}}" />
	<button class="item btn" data-field="add" data-limitation="{{limitation}}">+</button>
</div>